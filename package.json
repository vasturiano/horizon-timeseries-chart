{
  "name": "horizon-timeseries-chart",
  "version": "1.9.1",
  "description": "A chart to represent time-series data using multiple vertically layered horizon area plots",
  "unpkg": "dist/horizon-timeseries-chart.min.js",
  "jsdelivr": "dist/horizon-timeseries-chart.min.js",
  "main": "dist/horizon-timeseries-chart.common.js",
  "module": "dist/horizon-timeseries-chart.module.js",
  "types": "dist/horizon-timeseries-chart.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vasturiano/horizon-timeseries-chart.git"
  },
  "keywords": [
    "d3",
    "d3-module",
    "horizon",
    "timeseries",
    "chart",
    "svg"
  ],
  "author": {
    "name": "Vasco Asturiano",
    "url": "https://bl.ocks.org/vasturiano"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vasturiano/horizon-timeseries-chart/issues"
  },
  "homepage": "https://github.com/vasturiano/horizon-timeseries-chart",
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "dev": "rollup -w -c rollup.config.dev.js",
    "prepare": "npm run build"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "accessor-fn": "1",
    "d3-array": "1 - 3",
    "d3-axis": "1 - 3",
    "d3-horizon": "1",
    "d3-scale": "1 - 4",
    "d3-selection": "2 - 3",
    "d3-shape": "1 - 3",
    "d3-time": "1 - 3",
    "d3-time-format": "1 - 4",
    "d3-transition": "2 - 3",
    "d3-zoom": "1 - 3",
    "index-array-by": "1",
    "kapsule": "1",
    "lodash.memoize": "4"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.4",
    "@babel/preset-env": "^7.14.4",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "postcss": "^8.3.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.51.1",
    "rollup-plugin-dts": "^3.0.2",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.3.2"
  }
}
